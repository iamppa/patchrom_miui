<?xml version="1.0" encoding="utf-8"?>
<resources>
   
   <attr name="mediaViewStyle" format="reference" />   
   <declare-styleable name="MediaView">
      <attr name="mediaType">
          <enum name="poster" value="0" />
          <enum name="banner" value="1" />
          <enum name="tv" value="2" />
      </attr>
      <attr name="showText" format="boolean"/>
      <attr name="showMask" format="boolean"/>
   </declare-styleable>
   
   <attr name="ratingStarStyle" format="reference" />
   <declare-styleable name="RatingStar">
      <attr name="ratingStarType">
	       <enum name="small" value="0"></enum>
	       <enum name="normal" value="1"></enum>
   	  </attr>
   </declare-styleable>
   
   <attr name="v5_editmode_hide_icon" format="reference"/>
   <attr name="v5_editmode_unhide_icon" format="reference"/>
   
   <attr name="v5_delete_icon" format="reference"/>

</resources>